<%= link_to "メールアドレスでログインする", new_user_session_path, class: "btn loginPage_contents_btn" %>

モーダル
<div class="modal_window" , id="modal_window_new_room">
  <div class="modal_new_room">
    <br>
    <button type="button" class="btn modal_room_select_btn"><%= link_to "ホームに戻る", root_path %></button>
  </div>
</div>

デバイスのログイン画面
<% if devise_mapping.rememberable? %>
<div class="field">
  <%= f.check_box :remember_me %>
  <%= f.label :remember_me %>
</div>
<% end %>

<%#メンターリスト%>
<%= link_to mentors_list_up_profiles_path do %>
<div class="footer_btn col">
  <span class="fa-stack fa-lg">
    <i class="far fa-address-book fa-stack-1x"></i>
    <i class="far fa-circle fa-stack-2x text-primary"></i>
  </span>
</div>
<% end %>

<%#プロフィール%>
<% if user_signed_in? && Profile.all.where(user_id: current_user.id).blank? %>
<%= link_to new_profile_path do %>
<div class="footer_btn col">
  <span class="fa-stack fa-lg">
    <i class="far fa-id-card fa-stack-1x"></i>
    <i class="far fa-circle fa-stack-2x text-primary"></i>
  </span>
</div>
<% end %>
<% elsif user_signed_in? && Profile.all.where(user_id: current_user.id) != nil %>
<%= link_to profile_path(:user) do %>
<div class="footer_btn col">
  <span class="fa-stack fa-lg">
    <i class="far fa-id-card fa-stack-1x"></i>
    <i class="far fa-circle fa-stack-2x text-primary"></i>
  </span>
</div>
<% end %>
<% end %>

<%#戻る%>
<%= link_to :back do %>
<div class="footer_btn col">
  <span class="fa-stack fa-lg">
    <i class="fas fa-reply fa-stack-1x"></i>
    <i class="far fa-circle fa-stack-2x text-primary"></i>
  </span>
</div>
<% end %>

デバイス編集の下部にあったやつ
<h3>Cancel my account</h3>

<p>Unhappy?
  <%= button_to "Cancel my account", registration_path(resource_name), data: { confirm: "Are you sure?" }, method: :delete %>
</p>

<%= link_to "Back", :back %>

ホーム画面
<div class="card col-4" style="width: 18rem;">
  <div>
    <%= image_tag "admin_img.jpg", class:"card-img-top", alt:"..." %>
  </div>
  <h5 class="card-title">メンター入口</h5>
  <p class="card-text">メンターの方はこちら</p>
  <button type="button" class="btn btn-outline-primary"><%= link_to "Go", mentors_path %></button>
</div>

<%#雷マーク%>
<div class="col-3">
  <div class="row">
    <div class="col-2">
      <%= link_to "#" do %>
      <span class="fa-stack fa-lg">
        <i class="fas fa-bolt fa-stack-1x"></i>
        <i class="far fa-circle fa-stack-2x text-primary"></i>
      </span>
      <% end %>
    </div>

    <div class="card col-4" style="width: 18rem;">
      <div>
        <%= image_tag "change_img.jpg", class:"card-img-top", alt:"..." %>
      </div>
      <h5 class="card-title">管理人室</h5>
      <button type="button" class="btn btn-outline-primary"><%= link_to "Go", mentor_approval_mentors_path %></button>
      <p class="card-text"></p>
    </div>

    <%#歯車アイコン%>
<div id="modal_gear">
  <%= link_to "#" do %>
  <span class="fa-stack fa-lg">
    <i class="fas fa-cog fa-stack-1x"></i>
    <i class="far fa-circle fa-stack-2x text-primary"></i>
  </span>
  <% end %>
</div>
<%#歯車アイコン%>